  <head>
  <style>
    #map-canvas { width:950px; height:450px; }
    .layer-wizard-search-label { font-family: sans-serif };
  </style>
  <script type="text/javascript"
    src="http://maps.google.com/maps/api/js?sensor=false">
  </script>
  <script type="text/javascript">
    var map;
    var layer_0;
    var layer_1;
    function initialize() {
      map = new google.maps.Map(document.getElementById('map-canvas'), {
        center: new google.maps.LatLng(40.82665486385489, -74.50968057910158),
        zoom: 9
      });
      var style = [
        {
          featureType: 'all',
          elementType: 'all',
          stylers: [
            { saturation: -99 }
          ]
        }
      ];
      var styledMapType = new google.maps.StyledMapType(style, {
        map: map,
        name: 'Styled Map'
      });
      map.mapTypes.set('map-style', styledMapType);
      map.setMapTypeId('map-style');
      layer_0 = new google.maps.FusionTablesLayer({
        query: {
          select: "col2",
          from: "1rB8EDmRQJmhPeMVkXDqQM_TlUYj6Mj29WgQbvumZ"
        },
        map: map,
        styleId: 5,
        templateId: 6
      });
      layer_1 = new google.maps.FusionTablesLayer({
        query: {
          select: "col1",
          from: "1N50zz86UoZqWHOkhcvNf9446hVk1cN_CsXDDcehq"
        },
        map: map,
        styleId: 2,
        templateId: 2
      });
    }
    function changeMap_0() {
      var whereClause;
      var searchString = document.getElementById('search-string_0').value.replace(/'/g, "\\'");
      if (searchString != '--Select--') {
        whereClause = "'Center' = '" + searchString + "'";
      }
      layer_0.setOptions({
        query: {
          select: "col2",
          from: "1rB8EDmRQJmhPeMVkXDqQM_TlUYj6Mj29WgQbvumZ",
          where: whereClause
        }
      });
    }
    function changeMap_1() {
      var whereClause;
      var searchString = document.getElementById('search-string_1').value.replace(/'/g, "\\'");
      if (searchString != '--Select--') {
        whereClause = "'Name' = '" + searchString + "'";
      }
      layer_1.setOptions({
        query: {
          select: "col1",
          from: "1N50zz86UoZqWHOkhcvNf9446hVk1cN_CsXDDcehq",
          where: whereClause
        }
      });
    }
    google.maps.event.addDomListener(window, 'load', initialize);
  </script>
  </head>
